{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build",
			"command": "dotnet",
			"args": ["build"],
			"type": "process",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": "$msCompile",
			"options": {
				"cwd": "${workspaceFolder}/src/Helios365.Processor"
			}
		},
		{
			"label": "publish",
			"command": "dotnet",
			"args": ["publish", "--configuration", "Release"],
			"type": "process",
			"problemMatcher": "$msCompile",
			"options": {
				"cwd": "${workspaceFolder}/src/Helios365.Processor"
			}
		}
	]
}